{
    "root": true,
    "env": {
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "prettier/prettier": ["error"],
      "import/no-extraneous-dependencies": "off",
      "no-console": "off",
      "import/extensions": "off"
    },
    "globals": {
      "hre": "readonly",
      "ethers": "readonly",
      "describe": "readonly",
      "before": "readonly",
      "after": "readonly",
      "it": "readonly",
      "task": "readonly",
      "types": "readonly"
    }
}
